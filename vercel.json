{
    "version": 2,
    "builds": [
        {
            "src": "ask/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxiLambdaSize": "300mb", "runtime": "python3.9", "memory": "300mb"}
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1" 
        },
        {
            "src" : "/(.*)",
            "dest": "ask/wsgi.py"
        }
    ]
}